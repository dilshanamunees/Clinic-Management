 <div class="row p-3">
 <div class="col-md-4">
   <div class="card" style="width: 18rem;">
      <img src="/doctor-images/{{this.doctor._id}}.jpg" class="card-img-top img-thumbnail" style="width: 50%;">
                   
  <div class="card-body">
    <h3 class="card-title">{{this.doctor.name}}</h3>
    <h5 class="card-title">{{this.doctor.Specialised}}</h5>
    <h5 class="card-title">{{this.doctor.Field}}</h5>

    <ul class="list-group list-group-flush">
   <li class="list-group-item"> <a href="/doctor/view-appointments" >Appointments</a></li>
    <li class="list-group-item"><a href="/doctor/Bookings" >Booking</a></li>
    <li class="list-group-item"><a href="/doctor/mypatients" >My Patients</a></li>
    <li class="list-group-item"><a href="/doctor/results" >Results</a></li>
    <li class="list-group-item">Logout</li>
    
    
  </ul>
    
   </div>
</div>
</div>
<div class=" col-md-6">
    <div class="row">
          {{#each prescription}}
             
                <div class="card"style="height:20rem; width:15rem">
                  <div class="card-header">
                      <img src="/user-images/{{this.userID}}.jpg" class="card-img-top img-thumbnail" style="width: 35%;">
                   
                     <h5 >{{this.name}}</h5>
                     
                  </div>
                      <div class="card-body">
                     
                      <h6 class="card-subtitle mb-2 ">{{this.prescription}}</h6>
                       
                    
                      
                    </div>
                  </div>
                  {{/each}}
            </div>
            </div>
        
</div>